{
  "name": "pre-commit-eslint-and-prettier",
  "version": "0.0.1",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "prettier": "find src -name '*.js' -or -name '*.jsx' -or -name '*.ts' -or -name '*.tsx' | xargs prettier --write",
    "prepare": "npm run build; chmod +x dist/index.js"
  },
  "devDependencies": {
    "@types/eslint": "6.1.3",
    "@types/express": "4.17.2",
    "@types/lodash": "4.14.149",
    "@types/node": "13.1.4",
    "@types/prettier": "1.19.0",
    "eslint": "6.8.0",
    "import-sort-style-module": "6.0.0",
    "prettier": "2.3.2",
    "prettier-plugin-import-sort": "0.0.3",
    "ts-node": "8.5.4",
    "typescript": "3.7.4"
  },
  "peerDependencies": {
    "eslint": "*",
    "prettier": "*"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "karhu": "1.4.1",
    "lodash": "4.17.19"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "files": ["dist/"],
  "bin": "dist/index.js"
}
